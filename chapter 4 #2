var abc=[1,2,3,5]
newarray=[]
var reverseArray= function (array){
    
    for(i=array.length-1;i>=0;i--){
        newarray.push(array[i]);
    }
    return newarray;
}
reverseArray(abc);

arrayer=[1,2,3,4,5,6]
reverseArrayInPlace=function(array){
    
    for(i=0;i<=Math.floor((array.length/2)-1);i++){
        var mirror=array[i]
        array[i]=array[array.length-1-i];
        array[array.length-1-i]=mirror
    }
    return array
};
reverseArrayInPlace(arrayer);
